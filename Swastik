# MAKING SWASTIK BY TURTLE IN PYTHON
from turtle import *
speed (5)
penup()
goto (-100,100)
pendown()
right(180)
forward (50)
penup()
left(180)
forward(50)
pendown()
left(90)
forward (100)
right(90)
forward(50)
left(180)
forward(50)
left(90)
forward(50)
left(90)
forward(50)
right(90)
forward(50)
left(180)
forward(50)
left(90)
forward(100)
right(90)
forward(50)
